<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext" xmlns:pro="http://www.liquibase.org/xml/ns/pro" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd http://www.liquibase.org/xml/ns/pro http://www.liquibase.org/xml/ns/pro/liquibase-pro-4.1.xsd http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.1.xsd">
    <changeSet id="1" author="ATE">
        <createTable tableName="Bill">
            <column name="id" type="bigserial">
                <constraints nullable="false" primaryKey="true" primaryKeyName="bill_pkey"/>
            </column>
            <column name="date" type="timestamp"/>
            <column name="month" type="INTEGER"/>
            <column name="price" type="NUMERIC"/>
            <column name="payedbySophie" type="bool"/>
            <column name="payedbyAlex" type="bool"/>
            <column name="user" type="TEXT"/>
        </createTable>

        <createTable tableName="Closing">
            <column name="id" type="bigserial">
                <constraints nullable="false" primaryKey="true" primaryKeyName="closing_pkey"/>
            </column>
            <column name="date" type="timestamp"/>
            <column name="month" type="integer"/>
            <column name="year" type="int"/>
            <column name="monthString" type="text"/>
            <column name="probationSophie" type="numeric"/>
            <column name="probationAlex" type="numeric"/>
            <column name="probationCombined" type="numeric" />
        </createTable>
    </changeSet>
</databaseChangeLog>